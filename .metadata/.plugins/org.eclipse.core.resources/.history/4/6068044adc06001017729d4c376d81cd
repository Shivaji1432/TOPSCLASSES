package core_java;

public class StringChar {
public static void main(String[] args) {
	String s="shivshankae";
	char ch[]=s.toCharArray();
	for (int i = 0; i < ch.length; i++) {
		if(ch[i]=='s' || ch[i]=='h' ) {
			char ch1='r';
			char ch2='o';
			System.out.print(ch1+""+ch2);
		}else {
			System.out.print(ch[i]);
		}
	}
}
}
