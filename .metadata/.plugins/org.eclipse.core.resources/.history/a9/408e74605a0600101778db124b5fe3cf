package core_java;

public class StrintoStr {
public static void main(String[] args) {
	String s="hello in world";
	String []ss=s.split(" ");
	String s1="java";
	int index=1;
	for (int i = 0; i < ss.length; i++) {
		if(i==index) {
			System.out.print(s1+" ");
		}
		System.out.print(ss[i]+" ");
	}
}
}
