package assignment1;

import java.util.Scanner;

public class Q2Calc {
public static void main(String[] args) {
	Scanner sc= new Scanner(System.in);
	int a=sc.nextInt();
	int b=sc.nextInt();
	int c=0;
	System.out.println("calc");
	switch(c){
	case 1:
		System.out.println(a+b);
		break;
	case 2:
		System.out.println(a-b);
		break;
	case 3:
		System.out.println(a*b);
		break;
	case 4:
		System.out.println(a/b);
		break;
	default:
		System.out.println("error");
	}
}
}
