package core_java;

public class Countstring {
public static void main(String[] args) {
	String s="hello java hello python";
	String[] s1=s.split(" ");
	int count=0;
	for (int i = 0; i < s1.length; i++) {
		if(s1[i]=="hello") {
			count=count+1;
			System.out.println(count);
			count++;
		}else {
			System.out.println(s1[i]);
		}
	}
}
}
