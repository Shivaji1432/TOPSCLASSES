package core_java;

public class DataType {

	public static void main(String[] args) {
		int a=10;
		short b=15;
		long c=20;
		byte d=25;
		 float e=30.00f;
		 double f=35.00;
		 char g='a';
		 boolean h=false;
		 System.out.println(a+b+c+d+e+f+g);
		 System.out.println(h);
		 
		long l= a;
		System.out.println(l);
        short s= (short)a;// short cannot be int type
        int b1=(int)l;
        System.out.println(b1);
	}

}
